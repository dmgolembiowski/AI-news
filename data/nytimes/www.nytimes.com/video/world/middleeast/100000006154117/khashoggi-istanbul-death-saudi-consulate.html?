            searchNytimes: "https://query.nytimes.com/search/",
            searchVideo: "https://query.nytimes.com/search/video/",
            nyt5Host: "https://www.nytimes.com",
            nyt5MyAccount: "https://myaccount.nytimes.com",
            nyt5ProfileImage: "https://s3.amazonaws.com/pimage.timespeople.nytimes.com",
            nyt5GatewayHost: "https://www.nytimes.com",
            gatewayPageName: "gateway_times_talk.nytimes.com",
            branded: "https://brandedplaylist.nytimes.com",
            readingListApi: "https://reading-list.api.nytimes.com/reading-list/v2"
