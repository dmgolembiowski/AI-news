(function (w, d) {
  if (('XMLHttpRequest' in w && 'withCredentials' in new XMLHttpRequest()) || 'XDomainRequest' in w) {
    var el, cached = localStorage.getItem('games-fonts');
    if (cached && JSON.parse(cached).version == 1) {
      el = d.createElement('style');
      el.appendChild(d.createTextNode(JSON.parse(cached).d));
    } else {
      el = d.createElement('script');
      el.async = true;
      el.type = 'text/javascript';
      el.src = "/games-assets/fonts-4de645f2e0be0705c9c7219a041199ae.js";
      el.onload = function () { el.parentNode.removeChild(el); };
    }
    d.querySelector('head').appendChild(el);
  }
}(window, document));
