, args, result, timeout = null, previous = 0;\r\n            var later = function() {\r\n                previous = _now();\r\n                timeout = null;\r\n                result = func.apply(context, args);\r\n                if (!timeout) context = args = null;\r\n            };\r\n            return function() {\r\n                var now = _now(), remaining = wait - (now - previous);\r\n                context = this;\r\n                args = arguments;\r\n                if (remaining \u003C= 0 || remaining \u003E wait) {\r\n                    if (timeout) {\r\n                        clearTimeout(timeout);\r\n                        timeout = null;\r\n                    }\r\n                    previous = now;\r\n                    result = func.apply(context, args);\r\n                    if (!timeout) context = args = null;\r\n                } else if (!timeout && options.trailing !== false) {\r\n                    timeout = setTimeout(later, remaining);\r\n                }\r\n                return result;\r\n            };\r\n        }\r\n\r\n       \r\n    })();\r\n\u003C\u002Fscript\u003E\r\n\r\n\t\u003C!-- End ai2html - 2016-12-28 - 18:37 --\u003E\r\n\u003C\u002Fdiv\u003E\r\n\u003C!-- Pipeline: 2016-12-28-foxconn-map | December 28, 2016, 11:27PM | a46ac83d0f57888e84ca05b7ef840bedc08cf990 --\u003E\r\n\u003Cmeta name='interactivegraphic:height:300' content='1268'\u003E","compatibility":"NYT5","headline":{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2VmNzZkNGU4LTUxYWUtNThmYi1iYWIyLTJhZTkwNzZiZjVkOA==.sprinkledBody.content@filterEmpty.79.media.headline","typename":"CreativeWorkHeadline"},"advertisingProperties":{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2VmNzZkNGU4LTUxYWUtNThmYi1iYWIyLTJhZTkwNzZiZjVkOA==.sprinkledBody.content@filterEmpty.79.media.advertisingProperties","typename":"CreativeWorkAdvertisingProperties"},"displayProperties":{"type":"id","generated":true,"id":"$Article:QXJ0aW