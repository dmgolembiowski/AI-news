<script type="text/javascript">
  (function(){

    var cover = document.getElementById('g-cover-wrap');
    var video = document.getElementById('g-cover-video');
    var video_urls = JSON.parse(video.getAttribute('data-src'));
    var base_url = video_urls[0];

    var sizes = JSON.parse(video.getAttribute('data-sizes'));
    var width = window.innerWidth;
    var height = window.innerHeight;
    var aspect = width / height;
    var size = '640w';

    // if(aspect > 1) base_url += 'horz-'

    for (var i = 0; i < sizes.length; i++) {
      if(sizes[i] < width){
        size = sizes[i] + 'w';
      }
    }

    video.oncanplay = function(){
      // video.playbackRate = 0.5
      video.play()
    }
    video.onplay = function(){
      cover.classList.add('g-video-play');
    }

    if (video.canPlayType('video/mp4') != '') {
        video.src = base_url + size + '.mp4';
    } else if (video.canPlayType('video/webm') != '') {
        video.src = base_url + size + '.webm';
    }

    video.poster = aspect < 1 ? base_url + 640 + 'w.jpg' : base_url + 900 + 'w.jpg' ;

    if(window.IntersectionObserver){
      var observer = new IntersectionObserver(function(entries, observer) {
        entries.forEach(entry => {
          if(!entry.isIntersecting) {
            video.pause()
          } else {
            video.play()
          }
        });
      });
      observer.observe(video);
    }

  })()

</script>



